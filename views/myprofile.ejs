<%- include('./partials/header')-%>
<h1><%= user.fullname %></h1>
<div> 
    <h1>My Blogs</h1>
    <% if(blogs.length > 0){ %>
        <% blogs.forEach(blog => { %>
            <h3><%= blog.title %></h3>
            <h4><%= blog.description %></h4>
            <a href='/blogs/<%= blog._id %> '>Read More</a>
        <% }) %>
    <% } else { %>
        <p>You didn't publish any blogs yet</p>
    <%}%>
</div>
</body>
</html>